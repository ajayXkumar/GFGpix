{
    "name": "GFGpixi",
    "version": "1.0.1",
    "short_name": "MID",
    "description": "enable gfg profile page dark mode like leetcode",
    "permissions": [
        "declarativeContent",
        "storage",
        "activeTab"
    ],
    "content_scripts":[
        {
            "matches": [
                "https://auth.geeksforgeeks.org/user/*"
            ],
            "js": [
                "assets/scripts/contentScript.js"
            ]
        }
    ],
   
    
    "icons": {
        "16": "assets/images/makeitdark16.png",
        "32": "assets/images/makeitdark32.png",
        "48": "assets/images/makeitdark48.png",
        "128": "assets/images/makeitdark128.png"
    },

    "manifest_version": 2
}